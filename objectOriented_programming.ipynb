    "def num_oper():\n",
    "    c = int(input(\"Please enter your first number: \"))\n",
    "    f = int(input(\"Please enter your second number: \"))\n",
    "    \n",
    "    if c> f:\n",
    "        results = c-f\n",
    "    else:\n",
    "        results = c+f\n",
    "    return c, f, results\n",
    "\n",
    "print(\"You entered: \",c,\" and \", f,\"\", num_oper())"
